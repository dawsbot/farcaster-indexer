{
  "name": "farcaster-indexer",
  "author": "Greg Skriloff",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix --cache",
    "start": "bun run src/index.ts",
    "dev": "bun run start",
    "backfill": "bun run start -- --backfill",
    "kysely:migrate": "bun run src/db/migrator.ts",
    "test": "concurrently \"bun run lint\" \"bun run vitest -- --run\"",
    "prepare": "husky install",
    "vitest": "vitest"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.11.9",
    "bottleneck": "^2.19.5",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.4",
    "kysely": "^0.27.3",
    "p-limit": "^5.0.0",
    "pg": "^8.11.5",
    "pg-pool": "^3.6.2",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "viem": "^2.9.9"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.4",
    "@types/pg-pool": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "bun-types": "^1.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-justinanastos": "^1.3.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "vitest": "^1.4.0"
  }
}
